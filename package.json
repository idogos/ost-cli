{
  "name": "ost-cli",
  "version": "1.1.17",
  "description": "A mixture CLI for scaffolding Javascript projects",
  "bin": {
    "ost-cli": "./bin/ost-init"
  },
  "scripts": {
    "test:stage": "rm -rf ./build && cross-env NODE_ENV=stage node ./bin/ost init",
    "test:local": "rm -rf ./build && cross-env NODE_ENV=local node ./bin/ost init",
    "test-package:local": "rm -rf ./build && cross-env NODE_ENV=local node ./bin/ost-init --package",
    "test:unit": ""
  },
  "author": "leotw",
  "license": "MIT",
  "dependencies": {
    "async": "^2.6.1",
    "chalk": "^2.3.2",
    "commander": "^2.14.1",
    "compressing": "^1.4.0",
    "download-git-repo": "^1.0.2",
    "glob": "^7.1.2",
    "inquirer": "^5.1.0",
    "open": "^6.3.0",
    "ora": "^2.0.0",
    "ping": "^0.2.2",
    "randexp": "^0.5.2",
    "shelljs": "^0.8.1",
    "urllib": "^2.31.3"
  },
  "engines": {
    "node": ">=8.3.0"
  },
  "devDependencies": {
    "cross-env": "^5.2.0"
  }
}
